<h1 class="text-center">New Flat</h1>
<div class="container">

  <%= simple_form_for @flat do |f| %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :price %>
    <%= f.input :rooms %>

    <%= f.simple_fields_for Photo.new do |p| %>
      <%= p.input :cloudinary %>
      <%= p.input :cloudinary_cache, as: :hidden %>
    <% end %>

    <%= f.button :submit %>
  <% end %>
</div>
